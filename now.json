{
    "routes": [
        {
          "src": "/",
          "status": 301,
          "headers": { "Location": "/index" }
        },
        { "handle": "filesystem"}    
    ],
    "build": {
      "env": {
        "API_KEY": "@api-key"
      }
    },
    "env": {
      "API_KEY": "@api-key",
      "AUTH_DOMAIN":"@auth-domain",
      "DATABASE_URL":"@database-url",
      "PROJECT_ID":"@project-id",
      "STORAGE_BUCKET": "@storage-bucket",
      "MESSAGING_SENDER_ID": "@messaging-sender-id",
      "APP_ID": "@app-id",
      "MEASUREMENT_ID": "@measurement-id"
    },
    "regions": ["gru"]
}